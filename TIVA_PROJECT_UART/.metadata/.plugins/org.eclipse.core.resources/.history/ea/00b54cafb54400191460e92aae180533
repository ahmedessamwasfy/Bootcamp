
#ifndef UART_CFG_H_
#define UART_CFG_H_


#define F_CPU 16000000UL




#define BAUDRATE    38400 /* Baud rate */
#define NO_STOPBIT   UART_CONFIG_STOP_ONE
#define PARITYMODE  UART_CONFIG_PAR_NONE
#define DATASIZE    UART_CONFIG_WLEN_8


typedef struct{
    uint32 BaudRate;
    uint8 No_StopBits;
    uint8 Parity_Mode;
    uint8 Data_Size;
    uint8 SpeedMode;
    } UART_STRCT;



#endif /* UART_CFG_H_ */
